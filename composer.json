{
    "name": "runnels/web-starter",
    "description": "Boilerplate to get web project off the ground.",
    "require": {
        "fullpipe/twig-webpack-extension": "^4.0",
        "php-di/php-di": "^6.3",
        "slim/psr7": "^1.3",
        "slim/slim": "^4.7",
        "twig/twig": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "serve": "@php -S localhost:8888 -t public/"
    },
    "autoload":{
        "psr-4": {
            "App\\": "src/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "process-timeout": 0,
        "sort-packages": true
    }
}
